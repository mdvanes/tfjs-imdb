{
  "name": "basic-text-classification-js",
  "version": "1.0.0",
  "description": "Example TensorflowJS app for text classification",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.7",
    "http-server": "^0.11.1"
  },
  "scripts": {
    "watch": "./serve.sh",
    "build": "cp ../basic-text-classification-model/* ../docs/ && cp dist/metadata.json ../docs/ && parcel build index.html -d ../docs --public-url ./",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@polymer/lit-element": "^0.6.1",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-runtime": "6.26.0",
    "eslint": "^5.5.0",
    "eslint-config-google": "^0.10.0",
    "parcel-bundler": "^1.9.7"
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ]
}
