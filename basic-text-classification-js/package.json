{
  "name": "basic-text-classification-js",
  "version": "1.0.0-alpha.1",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.7"
  },
  "scripts": {
    "watch": "parcel --no-hmr index.html",
    "build": "mkdir dist && cp ../basic-text-classification-model/* dist/ && parcel build index.html --no-minify --public-url ./",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "6.26.0",
    "eslint": "^5.5.0",
    "eslint-config-google": "^0.10.0",
    "parcel-bundler": "^1.9.7"
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ]
}
